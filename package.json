{
  "name": "data-dumpster-diver",
  "version": "1.0.3",
  "description": "A desktop app to explore and visualize exported ChatGPT conversation data.",
  "license": "MIT",
  "author": "Ethan Thomason",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-run": "echo \"Error: no test specified\" && exit 1",
    "lint-and-fix": "npm run lint:fix && npm run format",
    "start": "electron .",
    "dev": "electron .",
    "web": "API_PORT=3001 node app.js",
    "lint": "eslint . --ext .js,.mjs",
    "lint:fix": "eslint . --ext .js,.mjs --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "migrate": "node data/migration.js data/conversations.json",
    "extract-assets": "node extract-assets-json.js",
    "dev-full": "API_PORT=3001 npm run web & API_PORT=3001 electron ."
  },
  "dependencies": {
    "axios": "^1.6.0",
    "decompress": "^4.2.1",
    "electron": "^35.7.5",
    "electron-log": "^5.4.3",
    "express": "^5.1.0",
    "fs": "^0.0.1-security",
    "marked": "^17.0.0",
    "multer": "^2.0.2",
    "path": "^0.12.7",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "sanitize-html": "^2.17.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4"
  }
}
