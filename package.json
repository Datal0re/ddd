{
  "name": "data-dumpster-diver",
  "version": "2.0.0-alpha",
  "description": "A CLI tool to process and explore exported ChatGPT conversation data.",
  "license": "MIT",
  "author": "Ethan Thomason",
  "main": "cli.js",
  "type": "commonjs",
  "bin": {
    "ddd": "./cli.js"
  },
  "scripts": {
    "start": "node cli.js",
    "dev": "nodemon cli.js",
    "lint": "eslint . --ext .js,.mjs",
    "lint:fix": "eslint . --ext .js,.mjs --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dump": "node data/dump.js data/conversations.json",
    "extract-assets": "node data/extract-assets-json.js"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "decompress": "^4.2.1",
    "marked": "^17.0.0",
    "sanitize-html": "^2.17.0",
    "uuid": "^13.0.0",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "nodemon": "^3.1.0"
  }
}
